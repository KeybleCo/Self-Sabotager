{
  "manifest_version": 3,
  "name": "Self Sabotager",
  "version": "1.0",
  "description": "Give Your Screen A Break!",
  "permissions": ["activeTab", "scripting", "tabs", "windows"],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "links.html"
  },
  "host_permissions": [
      "http://*/*",
      "https://*/*"
  ]
}